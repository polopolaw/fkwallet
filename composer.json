{
    "name": "polopolaw/fkwallet",
    "description": "Laravel package for fkwallet.io API integration",
    "type": "library",
    "license": "MIT",
    "version": "1.1.0",
    "authors": [
        {
            "name": "polopolaw"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "marcocesarato/php-conventional-changelog": "^1.17",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Polopolaw\\FKWallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Polopolaw\\FKWallet\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Polopolaw\\FKWallet\\FKWalletServiceProvider"
            ],
            "aliases": {
                "FKWallet": "Polopolaw\\FKWallet\\Facades\\FKWallet"
            }
        }
    },
    "scripts": {
        "test": "pest",
        "test-coverage": "pest --coverage",
        "analyse": "phpstan analyse",
        "changelog": "conventional-changelog",
        "release": "conventional-changelog --commit",
        "release:patch": "conventional-changelog --patch --commit",
        "release:minor": "conventional-changelog --minor --commit",
        "release:major": "conventional-changelog --major --commit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}